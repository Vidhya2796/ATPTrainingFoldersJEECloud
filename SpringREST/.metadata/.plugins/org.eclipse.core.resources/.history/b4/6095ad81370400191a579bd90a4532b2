package com.vidhyac.emp.dao;

import java.util.ArrayList;
import java.util.List;

import com.vidhyac.emp.model.Employee;


public class EmployeeDAO {
	
	public List<Employee> getAllUsers() {
		List<Employee> empList=new ArrayList<>();
		
		Employee e1=new Employee(1L, "Vidhya", "JEE", "Analyst", "20000")
		User e2=new User("Suraj", "sur");
		User e3=new User("Preethi", "pre");
		
		empList.add(e1);
		empList.add(e2);
		empList.add(e3);
		
		return empList;
	}
	
	
	
}
