package com.vidhyac.validation.dao;

import java.util.ArrayList;
import java.util.List;

import com.vidhyac.validation.model.User;

public class UserDAO {
	
	public List<User> getAllUsers() {
		List<User> usersList=new ArrayList<>();
		
		User u1=new User("Vidhya", "vid");
		User u2=new User("Suraj", "sur");
		User u3=new User("Preethi", "pre");
		
		usersList.add(u1);
		usersList.add(u2);
		usersList.add(u3);
		
		return usersList;
	}
}
