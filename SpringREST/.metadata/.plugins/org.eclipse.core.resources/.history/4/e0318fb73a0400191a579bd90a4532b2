package com.vidhyac.emp.controller;

import javax.ws.rs.FormParam;
import javax.ws.rs.Path;

import com.vidhyac.emp.service.EmployeeService;

@Path("employee")
public class EmployeeResource {
	private EmployeeService service;
	
	
	
	
	public EmployeeService getService() {
		return service;
	}

	public void setService(EmployeeService service) {
		this.service = service;
	}

	public EmployeeResource() {
		service=new EmployeeService();
	}
	
	public void addEmployee(@FormParam("id") long empId,@FormParam("name") String empName,@FormParam("dept") String empDept,@FormParam("design") String empDesign,@FormParam("sal") String empSal) {
		
		service.
		
		
		
		
	}
	
}
